<Window x:Class="GraphicSortAlgorithmsCollection.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="DataTextBox" HorizontalAlignment="Left" Height="22" Margin="10,25,0,0" TextWrapping="Wrap" Text="Data" VerticalAlignment="Top" Width="118"/>
        <Label x:Name="DataLabel" Content="Enter Data To Sort : " HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Width="118"/>
        <Button x:Name="SortButton" Content="Sort" HorizontalAlignment="Left" Margin="151,24,0,0" VerticalAlignment="Top" Width="75" Click="SortButton_Click"/>

    </Grid>
</Window>
